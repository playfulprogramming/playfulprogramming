    "date-fns": "^2.28.0",
    "dayjs": "^1.11.2",
    "disqus-react": "^1.1.3",
    "framer-motion": "^6.3.3",
    "gatsby-remark-embedder": "^6.0.0",
    "github-slugger": "^1.4.0",
    "gray-matter": "4.0.3",
    "jest-environment-jsdom": "^28.1.0",
    "junk": "^4.0.0",
    "lodash": "^4.17.21",
    "lunr": "^2.3.9",
    "next": "^12.1.6",
    "puppeteer-core": "^10.4.0",
    "react": "^18.1.0",
