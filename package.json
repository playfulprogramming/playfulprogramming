{
  "name": "unicorn-utterances-site",
  "private": true,
  "description": "Learning programming from magically majestic words",
  "version": "0.2.0",
  "bugs": {
    "url": "https://github.com/unicorn-utterances/unicorn-utterances/issues"
  },
  "homepage": "https://unicorn-utterances.com",
  "keywords": [
    "blog",
    "education",
    "programming"
  ],
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unicorn-utterances/unicorn-utterances.git"
  },
  "scripts": {
    "dev": "npm run lunr && env-cmd -f ./src/environments/dev.env next -p 9000",
    "debug": "npm run lunr && env-cmd -f ./src/environments/debug.env next -p 9000",
    "rss": "node --experimental-loader esbuild-node-loader src/build-scripts/gen-rss.ts",
    "lunr": "node --experimental-loader esbuild-node-loader src/build-scripts/lunr-index.ts",
    "build": "npm run lunr && npm run rss && next build && npm run sitemap",
    "build-beta": "env-cmd -f ./src/environments/beta.env npm run build",
    "sitemap": "node ./node_modules/next-sitemap/bin/next-sitemap",
    "start": "next start",
    "test": "jest",
    "typecheck": "tsc",
    "lint": "next lint",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@remark-embedder/core": "^2.0.0",
    "@remark-embedder/transformer-oembed": "^2.0.0",
    "batteries-not-included": "^0.1.0",
    "classnames": "2.3.1",
    "copy-webpack-plugin": "^9.0.1",
    "date-fns": "2.21.3",
    "dayjs": "^1.10.7",
    "disqus-react": "^1.1.2",
    "framer-motion": "^4.1.17",
    "gatsby-remark-embedder": "^5.0.0",
    "gray-matter": "4.0.3",
    "lunr": "^2.3.9",
    "next": "^12.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-medium-image-zoom": "^4.3.5",
    "react-paginate": "^8.0.3",
    "rehype-img-size": "github:unicorn-utterances/rehype-img-size#relative-path-full",
    "rehype-parse": "^8.0.3",
    "rehype-react": "^7.0.3",
    "rehype-slug": "github:unicorn-utterances/rehype-slug#built-custom-slug",
    "rehype-stringify": "^9.0.2",
    "remark-behead": "^2.3.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.0",
    "remark-rehype": "^10.0.1",
    "remark-retext": "^5.0.1",
    "remark-shiki-twoslash": "^3.0.5",
    "remark-stringify": "^10.0.1",
    "remark-unwrap-images": "^3.0.1",
    "retext-english": "^4.1.0",
    "sass": "^1.44.0",
    "sharp": "^0.29.3",
    "slash": "^4.0.0",
    "strip-markdown": "^5.0.0",
    "terser": "^4.8.0",
    "ts-util-helpers": "^1.1.0-alpha.2",
    "typescript": "^4.4.4",
    "unified": "^10.1.0",
    "unist-util-flat-filter": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@svgr/webpack": "^6.1.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/gtag.js": "0.0.8",
    "@types/jest": "^26.0.23",
    "@types/lunr": "^2.3.4",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/webpack-env": "^1.16.3",
    "babel-jest": "^27.4.5",
    "babel-plugin-transform-require-context": "^0.1.1",
    "env-cmd": "^10.1.0",
    "esbuild-node-loader": "^0.6.3",
    "eslint": "^7.32.0",
